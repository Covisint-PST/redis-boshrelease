{
    "service": {
        "name": "redis",
        <% if_p("redis.master") do %>
        "tags": ["slave"],
        <% end.else do %>
        "tags": ["master"],
        <% end %>
        "port": <%= p("redis.port") %>,
        "check": {
          "id": "redis-set",
          "name": "Redis set",
          "script": "redis-cli -p <%= p("redis.port") %> set consul-test \"$(date)\" || exit 2",
          "interval": "5s"
        }
    }
}
